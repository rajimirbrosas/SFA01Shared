    <validationRules>
        <fullName>Backdated_Close_Date</fullName>
        <active>false</active>
        <errorConditionFormula>(TODAY() - CloseDate) &gt; 7</errorConditionFormula>
        <errorDisplayField>CloseDate</errorDisplayField>
        <errorMessage>Expected Close Date cannot be backdated by 7 days.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Stage_Backward</fullName>
        <active>false</active>
        <errorConditionFormula>IF((NOT(ISPICKVAL(StageName, &quot;Closed Sales&quot;)) &amp;&amp; ISPICKVAL(PRIORVALUE(StageName),&quot;Closed Sales&quot;))||(ISPICKVAL(StageName, &quot;LOA&quot;) &amp;&amp; ISPICKVAL(PRIORVALUE(StageName),&quot;Infra Issues&quot;))||(ISPICKVAL(StageName, &quot;LOA&quot;) &amp;&amp; ISPICKVAL(PRIORVALUE(StageName),&quot;Order Placed&quot;))||(ISPICKVAL(StageName, &quot;Infra Issue&quot;) &amp;&amp; ISPICKVAL(PRIORVALUE(StageName),&quot;Order Placed&quot;)),TRUE, IF(Probability = 0, FALSE, Probability &lt; PRIORVALUE(Probability)))</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>Stage cannot be set to backward value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Close</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(StageName, &quot;Closed Sale&quot;)&amp;&amp;   ISBLANK(Billing_Effective_Date__c)</errorConditionFormula>
        <errorDisplayField>Billing_Effective_Date__c</errorDisplayField>
        <errorMessage>Billing Effective Date is required when closing an opportunity.</errorMessage>
    </validationRules>
